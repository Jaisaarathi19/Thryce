# Environment Variables for Email Integration
# Copy this file to .env and fill in your actual values
# Make sure to add .env to your .gitignore file

# EmailJS Configuration (Option 1 - Recommended)
REACT_APP_EMAILJS_SERVICE_ID=your_emailjs_service_id_here
REACT_APP_EMAILJS_TEMPLATE_ID=your_emailjs_template_id_here
REACT_APP_EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here

# Formspree Configuration (Option 2 - Alternative)
REACT_APP_FORMSPREE_FORM_ID=your_formspree_form_id_here

